{
  "name": "@metamask/sdk-react-native",
  "version": "0.5.6",
  "description": "A react component and react hooks to connect and use MetaMask",
  "homepage": "https://github.com/MetaMask/metamask-sdk#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/metamask-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/metamask-sdk.git"
  },
  "keywords": [
    "react",
    "typescript",
    "rollup",
    "metamask"
  ],
  "author": "MetaMask",
  "module": "dist/esm/index.js",
  "types": "dist/esm/src/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && rollup -c --bundleConfigAsCjs",
    "build:tsc": "tsc",
    "build:post-tsc": "echo 'N/A'",
    "build:pre-tsc": "echo 'N/A'",
    "clean": "rimraf dist",
    "dev": "rollup -c -w",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:changelog": "yarn auto-changelog validate",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path ../../.gitignore",
    "reset": "yarn clean && rimraf ./node_modules/",
    "test": "echo 'N/A'",
    "test:ci": "echo 'N/A'"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@lavamoat/allow-scripts": "^2.3.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "react-native": "^0.71.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@metamask/sdk-react": "workspace:*",
    "react-native-svg": "^13.10.0"
  },
  "lavamoat": {
    "allowScripts": {
      "@metamask/sdk-react>@metamask/sdk>eciesjs>secp256k1": false,
      "@metamask/sdk-react>wagmi>@wagmi/core>@wagmi/connectors>@coinbase/wallet-sdk>@solana/web3.js>bigint-buffer": false,
      "@metamask/sdk-react>wagmi>@wagmi/core>@wagmi/connectors>@coinbase/wallet-sdk>keccak": false,
      "bob>plato>eslint>es6-map>es5-ext": false,
      "react-native>react-devtools-core>ws>bufferutil": false,
      "react-native>react-devtools-core>ws>utf-8-validate": false,
      "@metamask/sdk-react>wagmi>@wagmi/core>@wagmi/connectors>@coinbase/wallet-sdk>@solana/web3.js>rpc-websockets>utf-8-validate": false
    }
  }
}
